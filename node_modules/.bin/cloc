#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/edutorstenwyoung/Apps/recipt-generator/node_modules/.pnpm/cloc@2.6.0-cloc/node_modules/cloc/lib/node_modules:/home/edutorstenwyoung/Apps/recipt-generator/node_modules/.pnpm/cloc@2.6.0-cloc/node_modules/cloc/node_modules:/home/edutorstenwyoung/Apps/recipt-generator/node_modules/.pnpm/cloc@2.6.0-cloc/node_modules:/home/edutorstenwyoung/Apps/recipt-generator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/edutorstenwyoung/Apps/recipt-generator/node_modules/.pnpm/cloc@2.6.0-cloc/node_modules/cloc/lib/node_modules:/home/edutorstenwyoung/Apps/recipt-generator/node_modules/.pnpm/cloc@2.6.0-cloc/node_modules/cloc/node_modules:/home/edutorstenwyoung/Apps/recipt-generator/node_modules/.pnpm/cloc@2.6.0-cloc/node_modules:/home/edutorstenwyoung/Apps/recipt-generator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/perl" ]; then
  exec "$basedir/perl"  "$basedir/../.pnpm/cloc@2.6.0-cloc/node_modules/cloc/lib/cloc" "$@"
else
  exec perl  "$basedir/../.pnpm/cloc@2.6.0-cloc/node_modules/cloc/lib/cloc" "$@"
fi
